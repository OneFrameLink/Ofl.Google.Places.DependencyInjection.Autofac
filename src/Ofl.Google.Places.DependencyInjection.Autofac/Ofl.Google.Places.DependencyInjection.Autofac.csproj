<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Ofl.Google.Places.Contract implementation registrations in Autofac.</Description>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Ofl.Google.Places.DependencyInjection.Autofac</AssemblyName>
    <PackageId>Ofl.Google.Places.DependencyInjection.Autofac</PackageId>
    <PackageProjectUrl>https://github.com/OneFrameLink/Ofl.Google.Places.DependencyInjection.Autofac</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OneFrameLink/Ofl.Google.Places.DependencyInjection.Autofac.git</RepositoryUrl>
	<Version>2.0.0</Version>
	<PackageTags>google-places google-places-api dependency-injection autofac</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="4.1.0" />
    <PackageReference Include="Ofl.Google.Places" Version="2.0.3" />
    <PackageReference Include="Ofl.Google.Places.Abstractions" Version="1.0.2" />
  </ItemGroup>

</Project>
